{
  "name": "@decsys/rating-scales",
  "version": "0.6.5",
  "description": "Reusable React Components for Rating Scales as used by the DECSYS Project",
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "watch": {
    "lib": {
      "patterns": [
        "src"
      ],
      "extensions": "js"
    },
    "dist": {
      "patterns": [
        "src"
      ],
      "extensions": "js"
    }
  },
  "scripts": {
    "lint": "eslint src/**/*.js",
    "build:lib": "babel src -d lib --no-comments --minified --delete-dir-on-start",
    "bundle:dev": "webpack --env.NODE_ENV=development",
    "bundle:prod": "webpack",
    "lib": "run-s lint build:lib",
    "dist": "run-s lint bundle:dev",
    "dist:prod": "run-s lint bundle:prod",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "styleguidist build",
    "watch": "npm-watch",
    "dev": "npm-run-all -s lint -p build:lib bundle:dev",
    "prod": "npm-run-all -s lint -p build:lib bundle:prod"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "@universityofnottingham",
      "plugin:react/recommended"
    ],
    "settings": {
      "react": {
        "version": "16.0"
      }
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decsys/rating-scales.git"
  },
  "author": "Jonathan Couldridge <jon@jon.gg>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/decsys/rating-scales/issues"
  },
  "homepage": "https://github.com/decsys/rating-scales#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/standalone": "^7.1.0",
    "@universityofnottingham/eslint-config": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "bootbox": "^4.4.0",
    "bootstrap": "^4.3.1",
    "clean-webpack-plugin": "^1.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.3",
    "jquery": "^3.3.1",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.4.0",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-styleguidist": "^8.0.4",
    "rimraf": "^2.6.2",
    "styled-components": "^4.1.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "peerDependencies": {
    "react": "~16",
    "styled-components": "~4"
  },
  "dependencies": {
    "colornames": "^1.1.1",
    "pixi.js": "^5.0.0-rc.3",
    "unit-value": "^1.0.1"
  }
}
