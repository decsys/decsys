{
  "name": "rating-scales",
  "version": "0.4.0",
  "description": "Reusable React Components for Rating Scales as used by the DECSYS Project",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "lib": "babel src -d lib --no-comments --minified --delete-dir-on-start",
    "dist:dev": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "pretty-quick --staged"
  },
  "eslintConfig": {
    "extends": [
      "@universityofnottingham",
      "plugin:react/recommended"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decsys/rating-scales.git"
  },
  "author": "Jonathan Couldridge <jon@jon.gg>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/decsys/rating-scales/issues"
  },
  "homepage": "https://github.com/decsys/rating-scales#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@universityofnottingham/eslint-config": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "eslint": "^5.8.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.3",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0",
    "rimraf": "^2.6.2",
    "webpack": "^4.25.1"
  }
}
